#!/usr/bin/python3

#
# Libraries
#
import subprocess

#
# Constants
#
CONFIG = "/home/pi/jonawifi/ap/wps.wpa2-psk.conf"

def activate():
	print("Activating access point...")
	subprocess.call(['hostapd',CONFIG])
	print("Access point activated.")

def deactivate():
	print("Dectivating access point...")
	print("Access point deactivated.")


activate();
